<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1093" id="life.brandonmartin.happyleaf" ios-CFBundleVersion="1093" version="0.1.9.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="BackgroundMode">
        <param name="browser-package" value="BackgroundMode" />
    </feature>
    <name>Happy Leaf</name>
    <description>
        Happy Leaf monitors OBDII data of the Nissan Leaf to provide drivers feedback and ease of range anxiety.
    </description>
    <author email="support@brandonmartin.life">
        Brandon Martin
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="loadUrlTimeoutValue" value="7000" />
    <preference name="orientation" value="all" />
</widget>
